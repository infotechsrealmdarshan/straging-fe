{
  "name": "pannellum-react",
  "version": "1.3.6",
  "description": "Pannellum React Component",
  "scripts": {
    "prebuild": "npm install --legacy-peer-deps",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 react-scripts build",
    "clean": "rimraf build",
    "start": "serve -s build",
    "dev": "cross-env HTTPS=true NODE_OPTIONS=--max-old-space-size=4096 react-scripts start",
    "test": "react-scripts test",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@mediapipe/tasks-vision": "^0.10.32",
    "@react-three/drei": "^9.99.0",
    "@react-three/fiber": "^8.15.16",
    "axios": "^1.13.2",
    "cross-env": "^7.0.3",
    "fabric": "^6.9.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-scripts": "^5.0.1",
    "serve": "^14.2.5",
    "three": "^0.160.0",
    "video.js": "7.18.1",
    "websocket-driver": "^0.7.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.8.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^1.2.0",
    "nwb": "^0.25.2"
  },
